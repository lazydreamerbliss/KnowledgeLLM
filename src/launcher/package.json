{
  "name": "kl-launcher",
  "version": "0.0.1",
  "description": "The launcher for KnowledgeLLM.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watchWeb": "webpack-dev-server --config-name web --mode development",
    "watchServer": "webpack --config-name server --watch --mode development",
    "watchElection": "tsc -p ./electron/ -w",
    "start": "electron ./electron/dist/main",
    "dev": "concurrently \"npm run watchWeb\" \"npm run watchServer\" \"npm run watchElection\"",
    "build": "webpack --config-name web --mode production && webpack --config-name server --mode production && tsc -p ./backend/"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.8",
    "babel-loader": "^9.1.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "electron": "^28.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "uuid": "^9.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  }
}
